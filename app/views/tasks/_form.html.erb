<%= form_with(model: message, local: true) do |f| %>
  <% if message.errors.any? %>
    <div id="error_explanation">
      <ul>
       <%= render 'layouts/error_messages' %>
      </ul>
    </div>
  <% end %>
 
  <%= f.label :status, 'ステータス' %>
  <%= f.text_field :status %>
 

  <%= f.label :content, 'タスク' %>
  <%= f.text_field :content %>

  <%= f.submit '投稿' %>
<% end %>